import java.io.*;
import java.util.*;
class rec
{
public static void main(String args[])
{
int rows;
Scanner s = new Scanner(System.in);
rows = s.nextInt();
for(int i=0;i<=rows;i++)
{
for(int j=0;j<=rows-i;j++)
{
System.out.print(" ");
}
int value=1;
for(int j=0;j<=i;j++)
{
System.out.print(value+"  ");
value = value * (i-j)/(j+1);
}
System.out.println();
}
}
}
